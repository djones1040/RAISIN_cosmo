B0;95;0c# Generate 10 data sets with 10k each after fitting, cuts and NN cut.

BATCH_INFO:  sbatch  $SBATCH_TEMPLATES/SBATCH_Midway2.TEMPLATE 20

# nominal generation

#GENVERSION: PS1_RAISIN_SNOOPY
#GENOPT:     GENMAG_SMEAR_MODELNAME G10
#GENOPT: ZVARIATION_FILE  $RAISIN_ROOT/cosmo/sim/inputs/PS1/SIMGEN_PS1SPEC_ZVARIATION.PAR

GENVERSION: PS1_RAISIN_SIM
GENOPT:     GENMAG_SMEAR_MODELNAME OIR
GENOPT: ZVARIATION_FILE  $RAISIN_ROOT/cosmo/sim/inputs/PS1/SIMGEN_PS1SPEC_ZVARIATION.PAR


ENDLIST_GENVERSION:

# specify sim-input files for snlc_sim.exe
SIMGEN_INFILE_Ia:       $RAISIN_ROOT/cosmo/sim/inputs/PS1/SIMGEN_PS1SPEC.INPUT
#NGEN_UNIT:  5
#CIDOFF:     5999386

# define required global items to ensure uniformity among all jobs
GENOPT_GLOBAL: GENRANGE_REDSHIFT 0.05 0.69
#ZRANGE:      0.5  0.69
GENPREFIX:   PS1_SPEC     # prefix of all data filenames
FORMAT_MASK: 48           # 2=TERSE    16=RanCID  32=FITS-FORMAT
PATH_SNDATA_SIM: $SCRATCH_SIMDIR

RANSEED_REPEAT:  10 123459
